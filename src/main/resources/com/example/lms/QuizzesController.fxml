<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lms.QuizzesController">
   <children>
      <AnchorPane fx:id="courseCol" layoutY="-2.0" prefHeight="400.0" prefWidth="146.0">
         <children>
            <Button fx:id="downloadBtn" layoutX="76.0" layoutY="352.0" mnemonicParsing="false" onAction="#Download" prefHeight="34.0" prefWidth="131.0" text="DOWNLOAD">
               <font>
                  <Font name="Cascadia Mono Regular" size="18.0" />
               </font>
            </Button>
         </children></AnchorPane>
      <ImageView fitHeight="69.0" fitWidth="156.0" layoutX="221.0">
         <image>
            <Image url="@../../../img/lms_new.jpg" />
         </image>
      </ImageView>
      <Button fx:id="quizzesBackButton" layoutX="547.0" layoutY="14.0" mnemonicParsing="false" onAction="#back_click" text="Back" />
      <Button fx:id="submitBtn" layoutX="254.0" layoutY="351.0" mnemonicParsing="false" onAction="#Submit" prefHeight="34.0" prefWidth="90.0" text="SUBMIT">
         <font>
            <Font name="Cascadia Mono Regular" size="18.0" />
         </font>
      </Button>
      <TableView fx:id="quiz_table_user" layoutX="2.0" layoutY="74.0" prefHeight="270.0" prefWidth="602.0">
         <columns>
            <TableColumn fx:id="file_name_col" editable="false" prefWidth="601.0" text="Quiz Title" />
         </columns>
      </TableView>
      <Button fx:id="submitBtn1" layoutX="369.0" layoutY="351.0" mnemonicParsing="false" onAction="#feedback" prefHeight="39.0" prefWidth="174.0" text="VIEW FEEDBACK">
         <font>
            <Font name="Cascadia Mono Regular" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
