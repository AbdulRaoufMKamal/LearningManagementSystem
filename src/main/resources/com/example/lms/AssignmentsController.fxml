<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lms.AssignmentsController">
   <children>
      <TableView fx:id="assignmentTable" layoutY="73.0" prefHeight="275.0" prefWidth="601.0">
         <columns>
            <TableColumn fx:id="courseCol" prefWidth="230.0" text="Course Name" />
            <TableColumn fx:id="assignmentCol" prefWidth="161.0" text="Assignment no." />
            <TableColumn fx:id="fileCol" prefWidth="209.0" text="File" />
         </columns>
      </TableView>
      <AnchorPane layoutY="-2.0" prefHeight="400.0" prefWidth="146.0" />
      <ImageView fitHeight="69.0" fitWidth="156.0" layoutX="221.0">
         <image>
            <Image url="@../../../img/lms_new.jpg" />
         </image>
      </ImageView>
      <Button fx:id="submitBtn" layoutX="369.0" layoutY="353.0" mnemonicParsing="false" onAction="#Submit" prefHeight="33.0" prefWidth="89.0" text="SUBMIT">
         <font>
            <Font name="Cascadia Mono Regular" size="18.0" />
         </font>
      </Button>
      <Button fx:id="downloadBtn" layoutX="166.0" layoutY="353.0" mnemonicParsing="false" onAction="#Download" prefHeight="33.0" prefWidth="110.0" text="DOWNLOAD">
         <font>
            <Font name="Cascadia Mono Regular" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
