<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lms.ViewSubmissions">
   <children>
      <TableView fx:id="sub_table" layoutY="59.0" prefHeight="275.0" prefWidth="601.0">
        <columns>
          <TableColumn fx:id="sub_id_col" prefWidth="208.0" text="Submission ID" />
          <TableColumn fx:id="file_name_col" prefWidth="263.0" text="File name" />
            <TableColumn fx:id="file_status_col" prefWidth="129.0" text="File Status" />
        </columns>
      </TableView>
      <Button fx:id="viewSubmissionsBackButton" layoutX="519.0" layoutY="23.0" mnemonicParsing="false" onAction="#back_click" text="BACK" />
      <Button fx:id="selectFileButton" layoutX="194.0" layoutY="347.0" mnemonicParsing="false" onAction="#selectFile" prefHeight="39.0" prefWidth="213.0" text="SELECT FILE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
