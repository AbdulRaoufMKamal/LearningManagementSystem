<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lms.ChooseCoursesController">
   <children>
      <TableView fx:id="courseTable" layoutX="26.0" layoutY="73.0" prefHeight="275.0" prefWidth="547.0">
         <columns>
            <TableColumn fx:id="course_name" editable="false" prefWidth="598.0" text="Your Courses" />
         </columns>
      </TableView>
      <AnchorPane layoutY="-2.0" prefHeight="400.0" prefWidth="146.0" />
      <ImageView fitHeight="69.0" fitWidth="156.0" layoutX="221.0">
         <image>
            <Image url="@../../../img/lms_new.jpg" />
         </image>
      </ImageView>
      <Button fx:id="studentSignOut" layoutX="512.0" layoutY="14.0" mnemonicParsing="false" onAction="#back_click" prefHeight="25.0" prefWidth="79.0" text="SIGN OUT" />
      <Button fx:id="selectCourseButton" layoutX="190.0" layoutY="354.0" mnemonicParsing="false" onAction="#selectCourse" prefHeight="39.0" prefWidth="235.0" text="SELECT COURSE">
         <font>
            <Font name="Cascadia Mono Regular" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
